<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.app.calendar.mapper.CalendarMapper"></mapper>

	<!-- 전체조회 -->
	<select id = "selectCalendarAll" resultType="CalendarVO">
		SELECT calendar_id
				, start_date
				, end_date
				, title
				, content
				, color
				, emp_id
		FROM calendar
		ORDER BY calendar_id
	</select>